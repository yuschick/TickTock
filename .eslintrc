{
  "extends": [
    "google",
    "plugin:react/recommended",
    "prettier",
    "prettier/react"
  ],
  "plugins": [
    "react",
    "prettier"
  ],
  "parserOptions": {
    "ecmaVersion": 2015,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "env": {
    "es6": true,
    "node": true
  },
  "rules": {
    "require-jsdoc": "off",
    "comma-dangle": [2,"always-multiline"],
    "no-cond-assign": [2,"except-parens"],
    "no-console": 1,
    "no-dupe-args": 2,
    "no-dupe-keys": 2,
    "no-duplicate-case": 2,
    "no-empty": 1,
    "no-extra-parens": [1,"all"],
    "no-extra-semi": 1,
    "no-sparse-arrays": 2,
    "use-isnan": 2,
    "eqeqeq": [2,"smart"],
    "no-alert": 2,
    "no-eval": 2,
    "no-fallthrough": 2,
    "no-loop-func": 2,
    "vars-on-top": 2,
    "yoda": [2,"never",{}],
    "no-unused-vars": 2,
    "comma-spacing": [2,{"after":true}],
    "new-cap": 2,
    "new-parens": 2,
    "no-mixed-spaces-and-tabs": [2,"smart-tabs"],
    "no-multiple-empty-lines": [2,{"max":3}],
    "no-nested-ternary": 2,
    "no-trailing-spaces": [2,{"skipBlankLines":false}],
    "padded-blocks": [2,"never"],
    "space-infix-ops": 2,
    "spaced-comment": [2,"always",{}],
    "arrow-parens": [2,"as-needed"],
    "arrow-spacing": [2,{"before":true,"after":true}],
    "constructor-super": 2,
    "no-const-assign": 2,
    "no-this-before-super": 2,
    "no-var": 2,
    "object-shorthand": [2,"always"],
    "prefer-const": 2,
    "prefer-spread": 1
  }
}
